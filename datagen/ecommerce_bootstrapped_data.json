[
    {
        "_meta": {
            "topic": "shops",
            "key": "id",
            "relationships": [
                {
                    "topic": "products",
                    "parent_field": "product_ids",
                    "child_field": "id",
                    "records_per": 10
                },
                {
                    "topic": "customers",
                    "parent_field": "customer_ids",
                    "child_field": "id",
                    "records_per": 3
                }
            ]
        },
        "id": "faker.number.int(100)",
        "name": "faker.company.name()",
        "product_ids": "faker.helpers.multiple((_, i) => i+1, {count: 10})",
        "customer_ids": "faker.helpers.multiple((_, i) => i+1, {count: 3})"
    },
    {
        "_meta": {
            "topic": "products",
            "key": "id",
            "relationships": [
                {
                    "topic": "orders",
                    "parent_field": "id",
                    "child_field": "product_id",
                    "records_per": 5
                }
            ]
        },
        "id": "this string can be anything since this field is determined by product.id",
        "title": "faker.commerce.productName()",
        "category": "faker.commerce.product()",
        "vendor": "faker.company.name()",
        "price": "faker.commerce.price({ min: 5, max: 100})",
        "description": "faker.commerce.productDescription()",
        "color": "faker.color.human()",
        "size": "faker.helpers.arrayElement(['S', 'M', 'L', 'XL'])",
        "material": "faker.commerce.productMaterial()"
    },
    {
        "_meta": {
            "topic": "customers",
            "key": "id",
            "relationships": [
                {
                    "topic": "orders",
                    "parent_field": "id",
                    "child_field": "customer_id",
                    "records_per": 4
                }
            ]
        },
        "id": "this string can be anything since this field is determined by customer.id",
        "name": "faker.person.fullName()",
        "email": "faker.internet.exampleEmail()",
        "address": "faker.location.streetAddress()",
        "city": "faker.location.city()",
        "state": "faker.location.state()",
        "zipcode": "faker.location.zipCode()"
    },
    {
        "_meta": {
            "topic": "orders",
            "key": "id"
        },
        "id": "faker.string.uuid()",
        "product_id": "faker.helpers.rangeToNumber({ min: 1, max: 10 })",
        "customer_id":  "faker.helpers.rangeToNumber({ min: 1, max: 3 })",
        "quantity": "faker.helpers.rangeToNumber({ min: 1, max: 10 })",
        "total_price": "faker.commerce.price({ min: 10, max: 250})",
        "created_at": "faker.date.past({ days: 30, refDate: '2024-01-01T00:00:00.000Z' })"
    }
]
